/* pages/dashborad/dashborad.wxss */

page {
  display: block;
  min-height: 100%;
  background-color: #f5f5f5;
}

.inner-container{
	width:100%;
	margin-top:32rpx;
}
.nav-item{
	width:339rpx;
	height:340rpx;
	padding:86rpx 0 0 0;
	margin-left:24rpx;
	box-sizing:border-box;
	background-color:#fff;
	box-shadow: 0px 0px 10px #DBDBDB;
}
.item-num{
	font-size:80rpx;
	line-height:112rpx;
	color:#17BEBB;
}
.item-title{
	font-size:32rpx;
	line-height:44rpx;
	color:#666666;
}
.item-unit{
	font-size:28rpx;
	line-height:40rpx;
	color:#DBDBDB;
}






