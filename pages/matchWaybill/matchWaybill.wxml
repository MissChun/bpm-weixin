<view class="match-waybill-container">
	<view class="top-bar-view">
		<view class="search-view">
			<view class="search-box">
				<view class="traggle-pick float-left">
					<picker mode="selector" bindchange="chooseField" range-key="label" value="{{choosedFieldIndex}}" range="{{fieldList}}">
					    <view class="float-left filed-picker">{{fieldList[choosedFieldIndex].label}}</view>
					    <view class="float-right traggle-down"><image src="/images/traggle_down.png" /></view>
					</picker>
				</view>
				<view class="float-left search-input-view">
					<input confirm-type="search" class="public-input" type="text" name="searchWord" placeholder="请输入" />
				</view>
			</view>
		</view>
	</view>
	<view class="waybill-list">
		<view class="waybill-item">
			<view class="waybill-item-title">
				<view class="waybill-item-title-content">
					<text class="float-left">博群安丘中</text>
					<text class="no-choose"></text>
					<icon class="choose-icon" type="success" color="#17BEBB" size="23"></icon>
				</view>
			</view>
			<view class="waybill-item-content">
				<view>计划卸车时间：2018-12-01</view>
				<view>计划卸车吨位</view>
				<view>收货人：明朗</view>
				<view>S1sefdf-马明</view>
			</view>
		</view>
		<view class="waybill-item">
			<view class="waybill-item-title">
				<view class="waybill-item-title-content">
					<text class="float-left">运单号：T8188832323</text>
					<text class="float-right">川A234</text>
				</view>
			</view>
			<view class="waybill-item-content">
				<view>计划卸车时间：2018-12-01</view>
				<view>计划卸车吨位</view>
				<view>收货人：明朗</view>
				<view>S1sefdf-马明</view>
			</view>
		</view>
		<view class="waybill-item">
			<view class="waybill-item-title">
				<view class="waybill-item-title-content">
					<text class="float-left">运单号：T8188832323</text>
					<text class="float-right">川A234</text>
				</view>
			</view>
			<view class="waybill-item-content">
				<view>计划卸车时间：2018-12-01</view>
				<view>计划卸车吨位</view>
				<view>收货人：明朗</view>
				<view>S1sefdf-马明</view>
			</view>
		</view>
	</view>
	<view class="buttom-confirm">
		<button  bindtap="confirmMatch" class="confirm-match-btn">提交卸货单</button>
	</view>
</view>

